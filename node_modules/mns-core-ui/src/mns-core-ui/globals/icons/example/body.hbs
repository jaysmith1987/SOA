<div class="container">
  <div class="icon-panel">
    <h2>General Icons</h2>
    <div class="row">
      {{#each data.icons}}
      <div class="col col-xs-6 col-sm-4 col-md-3 col-lg-2">
        <div class="swatch">
          <div class="swatch__bg--icon">
            <span class="mns-core__icon-{{{properties.name}}}"></span>
          </div>
          <div class="swatch__text swatch__text--icon">
            <span>{{{properties.name}}}</span>
            <span>mns-core__icon-{{{properties.name}}}</span>
          </div>
        </div>
      </div>
